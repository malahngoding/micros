{
  "name": "@malahngoding/micros",
  "version": "0.0.1",
  "description": "Malah Ngoding Micros",
  "private": true,
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  },
  "scripts": {
    "generate": "prisma generate",
    "dev": "concurrently \"pnpm run compile-watch\" \"pnpm run serve-watch\"",
    "build": "swc src -d --config-file .swcrc --out-dir build --copy-files",
    "compile-watch": "swc src -d --config-file .swcrc --out-dir build --copy-files --watch",
    "serve-watch": "nodemon build/index.js",
    "serve": "node build/index.js",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js}": [
      "npm run format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malahngoding/micros.git"
  },
  "author": "Malah Ngoding Bot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malahngoding/micros/issues"
  },
  "homepage": "https://github.com/malahngoding/micros#readme",
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "dependencies": {
    "@hashgraph/sdk": "2.0.13",
    "@prisma/client": "2.30.2",
    "bcrypt": "5.0.1",
    "chalk": "2.4.2",
    "crypto-js": "4.1.1",
    "dotenv": "10.0.0",
    "fastify": "3.20.2",
    "fastify-cors": "6.0.2",
    "fastify-helmet": "5.3.2",
    "fastify-plugin": "3.0.0",
    "mdps": "0.0.1",
    "vite": "2.6.13",
    "vite-plugin-node": "0.0.14",
    "zoya": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcrypt": "5.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.12",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.1",
    "jest": "^27.1.0",
    "lint-staged": "11.1.2",
    "prettier": "^2.3.2",
    "prisma": "2.30.2",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "@swc-node/core": "^1.8.0",
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.107",
    "chokidar": "^3.5.2",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.15"
  }
}
